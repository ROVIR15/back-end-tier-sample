{
  "name": "backend-api",
  "version": "1.0.0",
  "description": "backend api build with GraphQL in JS language",
  "main": "index.js",
  "scripts": {
    "build": "babel ./src --out-dir ./dist --source-maps --copy-files",
    "start": "npm run build && node dist/index.js",
    "dev": "nodemon --exec npx babel-node src/index.js",
    "prettier": "prettier --config ./.prettierrc --write \"**/*.js\"",
    "pretest": "eslint --ignore-path .gitignore .",
    "postinstall": "rm -rf dist && npm run build",
    "lint": "node prettier --write --check --config ./.prettierrc \"**/*.js\" && eslint --fix ./src",
    "release": "release-it patch --no-npm.publish"
  },
  "keywords": [
    "api"
  ],
  "author": "eko",
  "license": "ISC",
  "dependencies": {
    "@babel/cli": "^7.12.10",
    "@babel/core": "^7.12.10",
    "@babel/node": "^7.12.10",
    "@babel/preset-env": "^7.12.11",
    "apollo-engine": "^1.1.2",
    "apollo-server-express": "^2.19.2",
    "bcrypt-nodejs": "0.0.3",
    "body-parser": "^1.19.0",
    "cors": "^2.8.5",
    "dotenv": "^8.2.0",
    "express": "^4.17.1",
    "express-graphql": "^0.12.0",
    "express-validation": "^3.0.6",
    "graphql": "^15.4.0",
    "graphql-compose": "^7.24.1",
    "graphql-compose-connection": "^8.0.1",
    "graphql-compose-mongoose": "^9.0.0",
    "graphql-middleware": "^6.0.1",
    "graphql-tools": "^7.0.2",
    "graphql-upload": "^11.0.0",
    "joi": "^17.3.0",
    "jsonwebtoken": "^8.5.1",
    "mkdirp": "^1.0.4",
    "mongoose": "^5.11.12",
    "mongoose-bcrypt": "^1.9.0",
    "mongoose-timestamp": "^0.6.0",
    "passport": "^0.4.1",
    "passport-jwt": "^4.0.0",
    "passport-local": "^1.0.0",
    "restify": "^8.5.1",
    "shortid": "^2.2.16"
  },
  "devDependencies": {
    "babel-eslint": "^10.1.0",
    "babel-loader": "^8.2.2",
    "babel-preset-env": "^1.7.0",
    "eslint": "^7.18.0",
    "eslint-plugin-babel": "^5.3.1",
    "eslint-plugin-import": "^2.22.1",
    "eslint-plugin-node": "^11.1.0",
    "eslint-plugin-promise": "^4.2.1",
    "fs-extra": "^9.0.1",
    "nodemon": "^2.0.7",
    "prettier": "^2.2.1"
  }
}
